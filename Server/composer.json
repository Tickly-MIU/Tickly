{
    "name": "tickly/server",
    "description": "Backend server for Tickly application",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "guzzlehttp/guzzle": "^7.5",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "classmap": [
            "core/",
            "controllers/",
            "models/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    }
}